{
  "name": "vue-pattern-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fix": "npm run prettier && npm run lint:write && npm run dev",
    "build": "vite build",
    "build:check": "run-p type-check \"build-only {@}\" --",
    "build:fix": "npm run prettier && npm run lint:write && npm run build",
    "preview": "vite preview",
    "type:check": "vue-tsc --build --force",
    "prettier": "prettier --write src/",
    "prettier:write": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "fix": "npm run prettier && npm run lint:write"
  },
  "dependencies": {
    "@primevue/themes": "^4.0.0",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.0",
    "tailwindcss-primeui": "^0.3.4",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "prettier": "^3.0.3",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.0.11",
    "vite-plugin-pwa": "^0.20.0",
    "vue-tsc": "^1.8.27"
  }
}
